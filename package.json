{
  "name": "cf-package",
  "version": "0.1.0",
  "description": "Cloverfield Package Scaffold",
  "main": "build/cli.js",
  "bin": {
    "cf-package": "build/cli.js"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "babel-node `npm bin`/blue-tape ./test/**/*-test.js",
    "test:dev": "npm test | faucet",
    "cov": "npm run cov:clean && npm run cov:generate",
    "cov:clean": "rimraf coverage",
    "cov:generate": "babel-node `npm bin`/isparta cover --report text --report html test",
    "validate": "npm run lint && npm test",
    "audit": "nsp package",
    "check": "npm run lint && npm test && npm run audit && npm outdated --depth 0",
    "prebuild": "rimraf build",
    "build": "babel lib --out-dir build",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cloverfield-tools/cf-package.git"
  },
  "keywords": [
    "cloverfield",
    "scaffold",
    "cloverfield-scaffold",
    "package"
  ],
  "author": "Nik Butenko <nik@butenko.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cloverfield-tools/cf-package/issues"
  },
  "homepage": "https://github.com/cloverfield-tools/cf-package",
  "dependencies": {
    "babel": "^5.8.23",
    "glob": "^5.0.14",
    "handlebars": "^3.0.3",
    "mkdirp": "^0.5.1",
    "nomnom": "^1.8.1",
    "prompt": "^0.2.14"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.0",
    "blue-tape": "^0.1.10",
    "eslint": "^1.3.0",
    "faucet": "0.0.1",
    "isparta": "^3.0.3",
    "nsp": "^1.1.0",
    "precommit-hook": "^3.0.0",
    "rimraf": "^2.4.3",
    "sinon": "^1.16.1",
    "tap-xunit": "^1.1.1"
  },
  "pre-commit": [
    "lint"
  ]
}
